package com.pibs.model;

import java.io.Serializable;
import java.math.BigDecimal;
import java.sql.Timestamp;

public class Billing implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	private int id;
	private int patientCaseSystemid;
	private BigDecimal totalLaboratoryExamFee;
	private BigDecimal totalMedsupplyFee;
	private BigDecimal totalRadiologyFee;
	private BigDecimal totalSurgeryFee;
	private BigDecimal totalAdditionalServicesFee;
	private BigDecimal totalEquipmentFee;
	private BigDecimal totalRoomFee;
	private BigDecimal totalDoctorFee;
	private BigDecimal totalFee;
	private BigDecimal totalDiscountAmt;
	private BigDecimal totalTaxAmt;
	private BigDecimal totalBill;
	private Timestamp dateProcessed;
	private String remarks;
	private boolean cleared;
	
	private int createdBy;
	private Timestamp createdOn;
	private int modifiedBy;
	private Timestamp modifiedOn;
	private int version;
	private boolean active;
		
	public Billing() {}

