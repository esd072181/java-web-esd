package com.transport.form;

import org.apache.struts.validator.ValidatorForm;

/**
 * Can use the validator xml configuration but should have OWN formbean for the validation
 * if validate() method is override in child formbean, it is the priority, not the xml config
 * client side validation as javascripts can be configured, just add <html:javascript formName=""/>
 * in <head> of the page
 */
public class TransportFormBean extends ValidatorForm {
	
	private static final long serialVersionUID = 1L;
	
	String mode;

	public String getMode() {
		return mode;
	}

	public void setMode(String mode) {
		this.mode = mode;
	}
	
	

}
